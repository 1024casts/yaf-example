{
    "name": "phpcasts/yaf-skeleton",
    "description": "yaf-skeleton",
    "keywords": ["yaf-skeleton","yaf","skeleton"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "ext-yaf": "*",
        "monolog/monolog": "^1.13",
        "guzzlehttp/guzzle": "^6.2",
        "pimple/pimple": "^3.0",
        "illuminate/Database": "^5.5",
        "illuminate/events": "^5.5",
        "twig/twig": "^3.0@dev",
        "illuminate/view": "^5.5",
        "phpcasts/yaf-library": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\Exceptions\\": "app/exceptions",
            "App\\Services\\": "app/services",
            "App\\Defines\\": "app/defines"
        },
        "files": [
            "app/library/helpers.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
